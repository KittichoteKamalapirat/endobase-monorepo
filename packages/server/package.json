{
  "name": "nest-typescript-starter",
  "private": true,
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:showcase": "NODE_ENV=showcase && nest start",
    "dev:mac": "NODE_ENV=localhost nest start --watch",
    "dev:showcase": "NODE_ENV=showcase nest start --watch",
    "dev:win": "set NODE_ENV=hadyai&& nest start --watch",
    "dev:hadyai": "set NODE_ENV=hadyai&& nest start --watch",
    "execute": "ts-node ./src/console.ts",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "prod": "node dist/nodeService.js",
    "deploy": "yarn build && yarn prod",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm:gen:dev": "ts-node node_modules/typeorm/cli.js migration:generate -n migrationName -p -d src/migrations/stag -c forMigrationOfRemoteStagingDb",
    "typeorm:run:dev": "ts-node node_modules/typeorm/cli.js migration:run -c forMigrationOfRemoteStagingDb",
    "seed:run": "ts-node node_modules/typeorm-seeding/dist/cli.js seed --configName src/config/typeorm.config.migrations"
  },
  "dependencies": {
    "@nestjs/apollo": "^10.1.3",
    "@nestjs/common": "^9.1.2",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.1.2",
    "@nestjs/graphql": "^10.1.3",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/typeorm": "^9.0.1",
    "@ngneat/falso": "^6.2.0",
    "apollo-server-express": "^3.10.2",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "connect-redis": "^6.1.3",
    "dayjs": "^1.11.7",
    "express-session": "^1.17.3",
    "global": "^4.4.0",
    "graphql": "^16.6.0",
    "graphql-subscriptions": "^2.0.0",
    "ioredis": "^5.2.4",
    "modbus-serial": "^8.0.9",
    "nestjs-typeorm-paginate": "^4.0.2",
    "node-windows": "^1.0.0-beta.8",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.5",
    "serialport": "^10.4.0",
    "typeorm": "^0.3.10",
    "typeorm-seeding": "^1.6.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/connect-redis": "^0.0.19",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.5",
    "@types/jest": "^29.2.2",
    "@types/node": "^18.0.3",
    "@types/passport-local": "^1.0.34",
    "@types/redis": "^4.0.11",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.2",
    "prettier": "^2.7.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.5",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.8.2",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": [
      ".*\\.spec\\.ts$",
      ".*\\.test\\.ts$"
    ],
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}